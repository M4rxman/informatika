program massivy;
var A: array[1..10] of real;
    B: array[1..10] of real;
    x :real;
    k, i :integer;
begin
 for k:=1 to 10 do
   begin
   if (k mod 2 = 0) then 
    begin
    writeln ('A[',k,']');
    readln(A[k]); 
    end;
   end;
  for i:=1 to 10 do
   begin
   if (i mod 2 = 1) then 
    begin
    writeln('B[',i,']');
    readln(B[i]);
    end;
   end;
end.
